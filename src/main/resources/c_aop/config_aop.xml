	<aop:aspectj-autoproxy />

	<bean id="memoProcessor" class="com.dev4world.c_aop.MemoProcessor" />

	<!-- Definition for logging aspect -->
	<bean id="logging" class="com.dev4world.c_aop.Observer" />
</beans>
