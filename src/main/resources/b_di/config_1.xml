<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean id="cat" class="com.dev4world.b_di.CatMessageImpl"/>
	<bean id="dog" class="com.dev4world.b_di.DogMessageImpl"/>
	
	<bean id="myCat" class="com.dev4world.b_di.MyPet">
		<property name="pet" ref="cat"/>
	</bean>
	<bean id="myDog" class="com.dev4world.b_di.MyPet">
		<property name="pet" ref="dog"/>
	</bean>
</beans>
